# Copy this file to .env and fill in the values below

# Private key of the wallet signing transactions
PK=

# Alchemy RPC token for Polygon networks
RPC_TOKEN=

# Chain ID: 137 for Polygon mainnet, 80002 for Amoy testnet
CHAIN_ID=80002

# API credentials from Polymarket
CLOB_API_KEY=
CLOB_SECRET=
CLOB_PASS_PHRASE=

# Optional API base URL (defaults to https://clob.polymarket.com)
CLOB_API_URL=

# Optional WebSocket URL
WS_URL=
